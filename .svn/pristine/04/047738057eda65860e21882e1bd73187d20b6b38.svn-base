                    <span class="overview-box-count">5251</span>
                    <span class="overview-box-badge">25%</span>
                    <p:graphicImage name="images/dashboard/chart-4.svg" library="barcelona-layout" />
                </div>
            </div>

            <div class="ui-g-12 ui-lg-4">
                <div class="user-card ui-shadow-content">
                    <div class="user-card-header">
                        <p:graphicImage name="images/dashboard/user-card-header.png" library="barcelona-layout" />
                    </div>
                    <div class="user-card-content">
                        <p:graphicImage name="images/profile-image.png" library="barcelona-layout" />
                        <p:commandButton id="user-button" type="button" icon="ui-icon-plus" styleClass="secondary"/>

                        <p:menu overlay="true" trigger="user-button" my="left top" at="left bottom">
                            <p:menuitem value="Save" icon="ui-icon-save" url="#"/>
                            <p:menuitem value="Update" icon="ui-icon-edit" url="#"/>
                            <p:menuitem value="Delete" icon="ui-icon-trash" url="#"/>
                        </p:menu>

                        <div class="user-card-name">
                            <span>Claire White</span>
                        </div>

                        <div class="user-detail">
                            <ul>
                                <li class="clearfix">
                                    <div class="project-progressbar">
                                        <div class="project-progressbar-value" style="width: 50%"></div>
                                    </div>
                                    <i class="fa fa-tasks fa-fw"></i>
                                    <span class="project-title">Tasks</span>
                                    <span class="project-detail">3 open</span>
                                </li>
                                <li class="clearfix">
                                    <div class="project-progressbar">
                                        <div class="project-progressbar-value" style="width: 20%"></div>
                                    </div>
                                    <i class="fa fa-usd fa-fw"></i>
                                    <span class="project-title">Revenue</span>
                                    <span class="project-detail">+20%</span>
                                </li>
                                <li class="clearfix">
                                    <div class="project-progressbar">
